#!/usr/bin/env node

var command = require('new-command')({
  p: 'port',
  n: 'hostname',
  o: 'out'
});

require('default-debug')('brick:*');

var options = require('../lib/node/options');

if (command.port) options.port(command.port);
if (command.out) options.out(command.out);

require('../lib/node/cli')(command);
